            </Label>
            <TextField fx:id="tLocation" layoutX="179.0" layoutY="167.0" prefHeight="25.0" prefWidth="185.0" />
            <Button layoutX="232.0" layoutY="214.0" mnemonicParsing="false" onAction="#assignReceptionist" text="register">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font>
            </Button>
            <Button layoutX="14.0" layoutY="278.0" mnemonicParsing="false" onAction="#prevMenu" text="previous" />
            <Label fx:id="employeeAdded" layoutX="150.0" layoutY="273.0" prefHeight="35.0" prefWidth="287.0" textFill="#484a20">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
